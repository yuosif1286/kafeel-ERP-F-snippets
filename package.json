{
  "name": "alkafeel-erp-f-snippets",
  "displayName": "alkafeel-ERP-F-snippets",
  "description": "Custom AlKafeel Front End ERP  UI-Kit snippets",
  "version": "1.0.3",
  "engines": {
    "vscode": "^1.105.0"
  },
  "private":true,
  "scripts": {
    "commit": "cz",
    "prepare": "husky install",
    "release": "semantic-release"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      }
    ]
  },
  "icon": "logo.png",
  "publisher": "yuosifraedhashem",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuosif1286/kafeel-ERP-F-snippets.git"
  },
"release": {
  "branches": [
    "master",
    {
      "name": "alpha",
      "prerelease": true
    },
    {
      "name": "beta",
      "prerelease": true
    }
  ],
    "repositoryUrl": "git@github.com:yuosif1286/kafeel-ERP-F-snippets.git",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
   [
      "@semantic-release/npm",
      {
        "npmPublish": false,
        "tarballDir": "dist"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["package.json", "CHANGELOG.md"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  ]
}
,
"dependencies": {
  "@semantic-release/changelog": "^6.0.3",
  "@semantic-release/commit-analyzer": "^13.0.1",
  "@semantic-release/git": "^10.0.1",
  "@semantic-release/github": "^11.0.6",
  "@semantic-release/npm": "^12.0.2",
  "@semantic-release/release-notes-generator": "^14.1.0",
  "semantic-release": "^25.0.0"
},
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
 
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
