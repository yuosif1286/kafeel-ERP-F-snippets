        "mock request get with pagination": {
        "prefix": "mk-get-with-pagination",
        "body": [
            "",
            "  http.get(('/api/apps/food/types'), ({ request }) => {",
            "\tconst url = new URL(request.url)",
            "",
            "\tconst q = url.searchParams.get('q')",
            "\tconst sortBy = url.searchParams.get('sortBy')",
            "\tconst orderBy = url.searchParams.get('orderBy')",
            "\tconst itemsPerPage = url.searchParams.get('itemsPerPage')",
            "\tconst page = url.searchParams.get('page')",
            "",
            "\tconst parsedSortBy = destr(sortBy)",
            "\tconst sortByLocal = is.string(parsedSortBy) ? parsedSortBy : ''",
            "",
            "\tconst parsedOrderBy = destr(orderBy)",
            "\tconst orderByLocal = is.string(parsedOrderBy) ? parsedOrderBy : ''",
            "",
            "\tconst parsedItemsPerPage = destr(itemsPerPage)",
            "\tconst parsedPage = destr(page)",
            "",
            "\tconst itemsPerPageLocal = is.number(parsedItemsPerPage) ? parsedItemsPerPage : 10",
            "\tconst pageLocal = is.number(parsedPage) ? parsedPage : 1",
            "",
            "\tconst searchQuery = is.string(q) ? q : undefined",
            "\tconst queryLowered = (searchQuery ?? '').toString().toLowerCase()",
            "",
            "\tconst filteredCustomers = db.foodTypeData.filter(pet => {",
            "\t  return (",
            "\t\t(pet.name && pet.name.toLowerCase().includes(queryLowered))",
            "\t\t|| (pet.description && pet.description.toLowerCase().includes(queryLowered))",
            "\t  )",
            "\t}).reverse()",
            "",
            "\t// Sort Customers",
            "\tif (sortByLocal) {",
            "\t  if (sortByLocal === 'name') {",
            "\t\tfilteredCustomers.sort((a, b) => {",
            "\t\t  if (orderByLocal === 'asc')",
            "\t\t\treturn a.name.localeCompare(b.name)",
            "",
            "\t\t  return b.description!.localeCompare(a.description!)",
            "\t\t})",
            "\t  }",
            "\t}",
            "",
            "",
            "",
            "\treturn HttpResponse.json(",
            "\t  {",
            "\t\tfoods: paginateArray(filteredCustomers, itemsPerPageLocal, pageLocal), total: filteredCustomers.length,",
            "\t  },",
            "\t  {",
            "\t\tstatus: 200,",
            "\t  })",
            "  }),"
        ],
        "description": "this will generating  method mock data with pagination endpoint"
    },
        "mock request get ": {
        "prefix": "mk-get",
        "body": [
            "  http.get(('/api/apps/food/type/drop-down'), ({ request }) => {",
            "",
            "\tconst url = new URL(request.url)",
            "",
            "\tconst q = url.searchParams.get('q')",
            "",
            "",
            "\tconst searchQuery = q ? q : undefined",
            "\tconst queryLowered = (searchQuery ?? '').toString().toLowerCase()",
            "",
            "\tconst filteredFoodTypes = db.foodTypeData.filter(foodType => {",
            "\t  return (",
            "\t\t(foodType.name && foodType.name.toLowerCase().includes(queryLowered))",
            "\t  )",
            "\t}).reverse()",
            "",
            "\treturn HttpResponse.json(",
            "\t  { data: filteredFoodTypes },",
            "\t  { status: 200 }",
            "\t)",
            "  }),"
        ],
        "description": "this will generating method mock data endpoint"
    },
        "mock request post endpoint": {
        "prefix": "mk-post",
        "body": [
            "",
            "  http.post('/api/apps/pets', async ({ request }) => {",
            "\ttry {",
            "\t  const body = await request.json()",
            " ",
            "\t  if (!body || typeof body !== 'object') {",
            "\t\treturn HttpResponse.json(",
            "\t\t  { message: 'Name is required' },",
            "\t\t  { status: 400 }",
            "\t\t)",
            "\t  }",
            "\t  const newPet= body as Pet",
            "\t  // Generate a new ID (simple auto-increment)",
            "\t  const newId = (db.petData.length + 1)",
            "\t\tnewPet.id = newId",
            "",
            "\t  // Add to mock database",
            "\t  db.petData.push(newPet)",
            "",
            "\t  // Return success response",
            "\t  return HttpResponse.json(",
            "\t\t{ message: 'Brand added successfully', data: newPet },",
            "\t\t{ status: 201 }",
            "\t  )",
            "\t} catch (error) {",
            "\t  return HttpResponse.json(",
            "\t\t{ message: 'Invalid JSON body' },",
            "\t\t{ status: 400 }",
            "\t  )",
            "\t}",
            "  }),"
        ],
        "description": "this will generating mock data method post endpoint "
    },
        "mock request delete endpoint": {
        "prefix": "mk-delete",
        "body": [
            "http.delete('/api/apps/pets/:id', ({ params }) => {",
            "  const id =params.id as unknown as number",
            "  // Validate ID",
            "  if (isNaN(id)) {",
            "\treturn HttpResponse.json(",
            "\t  { message: 'Invalid ID' },",
            "\t  { status: 400 }",
            "\t)",
            "  }",
            "",
            "  // Find index of pet in mock database",
            "  const index = db.petData.findIndex(pet => pet.id === id)",
            "  if (index === -1) {",
            "\treturn HttpResponse.json(",
            "\t  { message: 'Pet not found' },",
            "\t  { status: 404 }",
            "\t)",
            "  }",
            "",
            "  // Remove the pet",
            "  const deletedPet = db.petData.splice(index, 1)[0]",
            "",
            "  return HttpResponse.json(",
            "\t{ message: 'Pet deleted successfully', data: deletedPet },",
            "\t{ status: 200 }",
            "  )",
            "}),"
        ],
        "description": "this will generating method delete endpoint"
    },
        "mock request put endpoint": {
        "prefix": "mk-put",
        "body": [
            "http.put('/api/apps/pets/:id', async ({ params, request }) => {",
            "  const id = Number(params.id)",
            "",
            "  // Validate ID",
            "  if (isNaN(id)) {",
            "\treturn HttpResponse.json(",
            "\t  { message: 'Invalid ID' },",
            "\t  { status: 400 }",
            "\t)",
            "  }",
            "",
            "  // Parse request body",
            "  const updatedData = await request.json()",
            "",
            "  // Find existing pet",
            "  const petIndex = db.petData.findIndex(pet => pet.id === id)",
            "  if (petIndex === -1) {",
            "\treturn HttpResponse.json(",
            "\t  { message: 'Pet not found' },",
            "\t  { status: 404 }",
            "\t)",
            "  }",
            "",
            "  // Update the pet data (merge existing and new)",
            "  db.petData[petIndex] = { ...db.petData[petIndex], ...updatedData  as Pet }",
            "",
            "  return HttpResponse.json(",
            "\t{",
            "\t  message: 'Pet updated successfully',",
            "\t  data: db.petData[petIndex],",
            "\t},",
            "\t{ status: 200 } ",
            "  )",
            "})"
        ],
        "description": "this will generating method Put endpoint"
    }
    
